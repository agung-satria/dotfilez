#!/bin/sh

function rundiscord() {
  if ! command -v eww &> /dev/null
  then
    notify-send "eww is not installed"
    exit
  else
    discord
  fi
}

    case $BLOCK_BUTTON in
      # 1) eww open eww ;;
      # 3) eww close eww ;;
      1) $HOME/.config/eww/slider/launch_slider ;;
      6) "$TERMINAL" -e "$EDITOR" "$0" ;;
    esac; echo " ﳟ   "
